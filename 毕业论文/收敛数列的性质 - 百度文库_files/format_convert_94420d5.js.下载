define("wkview:widget/format_convert/dialog_down_doc/format_convert.js",function(o,i,t){var e=o("wkcommon:widget/lib/doT/doT.min.js"),n=o("wkcommon:widget/ui/js_core/log/log.js"),c={1:"pdf",2:"ppt",3:"word"};t.exports={init:function(o,i){var t,n,d=i.vipInfo,a=i.transType;for(var r in a)if(a.hasOwnProperty(r)){t=r,n=a[r];break}if(t&&n.length){var s={docTypeMap:c,docTypeOri:t,docTypeCandidates:n,vipInfo:d},p=e.template('<div class="format-convert-content">\n<div class="format-convert-text">\n<p>\u8f6c\u4e3a\u5176\u4ed6\u683c\u5f0f</p>\n<i class="vip-tag-icon"></i>\n<i class="arrow"></i>\n</div>\n<div class="format-convert-list">\n<div class="format-convert-item"\n            data-doc-type="{{=it.docTypeOri}}"\n        >\n<i class="doc-icon doc-icon-{{=it.docTypeMap[it.docTypeOri]}}"></i>\n<p>{{=it.docTypeMap[it.docTypeOri]}}\u539f\u683c\u5f0f</p>\n</div>\n{{~ it.docTypeCandidates :item:i }}<div class="format-convert-item" data-doc-type="{{=item}}">\n<i class="doc-icon doc-icon-{{=it.docTypeMap[item]}}"></i>\n<p>\u8f6c\u4e3a{{=it.docTypeMap[item]}}\u683c\u5f0f</p>\n<i class="vip-tag-icon"></i>\n</div>\n{{~}}</div>\n</div>')(s);o.html(p),this.bindEvent(o,s)}},bindEvent:function(o,i){var t=i.vipInfo,e=i.docTypeMap,c=+i.docTypeOri;o.on("mouseenter",".format-convert-content",function(){n.xsend(1,101966),$(this).addClass("hover"),$(".format-convert-list",o).show()}).on("mouseleave",".format-convert-content",function(){$(this).removeClass("hover"),$(".format-convert-list",o).hide()}).on("click",".format-convert-item",function(){var i=$(this),n=+i.data("docType"),d=e[n];if(document.downloadForm&&(window.dataFormatConvert={docTypeOrigin:+c,docTypeConvert:+n}),t.isVip)$(window).trigger("switch-format-doc-type",{docTypeText:d});else if(n===c)$(window).trigger("switch-format-doc-type",{docTypeText:d});else{var a="https://wenku.baidu.com/cashier/browse/jiaoyuvipcashier?"+$.param({cashier_code:"pcview_download_convert"});window.open(a)}$(".format-convert-list",o).hide()})}}});