define("question-new:widget/ask/pc-exp/ask-img",function(i){var e=i("common:widget/lib/jquery/jquery"),n=i("question-new:widget/read-opt/img-preview/preview");e(".q-img-wp").each(function(i,n){var o=e(n),t=o.find(".q-img-ul"),r=o.find(".q-img-li"),l=o.find(".q-img-con"),p=t.width(),a=l.width();a>=p&&(o.find(".q-img-prev-wp").remove(),o.find(".q-img-next-wp").remove()),o.find(".q-img-next").on("click",function(){var i=t.position().left;t.animate(p+i>=2*a?{left:"-"+(a-i)+"px"}:{left:"-"+(p-a)+"px"})}),o.find(".q-img-prev").on("click",function(){var i=t.position().left;t.animate(-a>=i?{left:i+a+"px"}:{left:"0px"})}),r.on("click",function(){var i=e(this).find(".q-img-item").clone(),n=e('<div class="q-img-fullscreen-wraper"></div>');n.append(i),e("body").append(n),e(this).closest(".replyask-box").size()||e("body").addClass("q-img-body-noscroll"),i.css({marginLeft:-i.width()/2+"px",marginTop:-i.height()/2+"px"}),n.click(function(i){i.stopPropagation(),e("body").removeClass("q-img-body-noscroll"),e(this).remove()})}),e.trim(o.prev().html()).replace(/<br>/g,"")||o.prev("pre").remove()}),"view"===F.context("pageType")&&(e(".replyask-box .ikqb_img_alink").click=null,new n({$el:".replyask-box .ikqb_img_alink",imgAttr:"href"}))});